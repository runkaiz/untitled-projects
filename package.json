{
  "name": "untitled-project",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "devopen": "vite dev --open",
    "devhost": "vite dev --host",
    "build": "vite build",
    "prepare": "svelte-kit sync",
    "package": "vite package",
    "preview": "vite preview",
    "database": "pscale connect untitled-db main --port 3309",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vercel-postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "vercel-build": "prisma generate && pnpm build && pnpm vercel-postbuild",
    "studio": "npx prisma studio",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.39",
    "@storybook/svelte": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "^1.0.0-next.360",
    "@tailwindcss/typography": "^0.5.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-svelte3": "^4.0.0",
    "mdsvex": "^0.10.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "prisma": "^4.0.0",
    "stylelint-config-recommended": "^8.0.0",
    "svelte": "^3.49.0",
    "svelte-keydown": "^0.5.0",
    "svelte-loader": "^3.1.3",
    "tailwindcss": "^3.0.16",
    "vite": "^2.9.13"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@storybook/addon-svelte-csf": "^2.0.5",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "highlight.js": "^11.5.1",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^13.0.1",
    "nock": "^13.2.2",
    "svelte-markdown": "^0.2.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-transition-classes": "^0.1.1"
  }
}
