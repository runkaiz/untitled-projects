{
	"name": "untitled-project",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"devopen": "vite dev --open",
		"devhost": "vite dev --host",
		"build": "vite build",
		"prepare": "svelte-kit sync",
		"package": "vite package",
		"preview": "vite preview",
		"database": "pscale connect untitled-db main --port 3309",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"vercel-postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
		"vercel-build": "prisma generate && pnpm build && pnpm vercel-postbuild",
		"studio": "npx prisma studio"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "^1.0.0-next.360",
		"@tailwindcss/typography": "^0.5.1",
		"autoprefixer": "^10.4.2",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.5",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"prisma": "^4.0.0",
		"stylelint-config-recommended": "^8.0.0",
		"svelte": "^3.48.0",
		"svelte-keydown": "^0.5.0",
		"tailwindcss": "^3.0.16",
		"mdsvex": "^0.10.5",
		"vite": "^2.9.13"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.0.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"bcryptjs": "^2.4.3",
		"cookie": "^0.5.0",
		"highlight.js": "^11.5.1",
		"jsonwebtoken": "^8.5.1",
		"markdown-it": "^13.0.1",
		"nock": "^13.2.2",
		"svelte-markdown": "^0.2.2",
		"svelte-transition-classes": "^0.1.1"
	}
}
