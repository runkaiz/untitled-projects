		"autoprefixer": "^10.4.2",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.5",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"prisma": "^3.14.0",
		"stylelint-config-recommended": "^8.0.0",
